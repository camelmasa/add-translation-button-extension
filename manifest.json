{
  "manifest_version": 2,
  "name": "Add Translation Button",
  "description": "This extension adds one click translation button",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*",
        "https://news.ycombinator.com/item*",
        "https://stackoverflow.com/questions/*",
        "https://www.reddit.com/r/*"
      ],
      "js": ["dist/index.js"]
    }
  ]
}
